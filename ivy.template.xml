<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" xmlns:e="http://ant.apache.org/ivy/extra" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file:/D:/Projects/ext/ivy-2.3.0/ivy.xsd">
    <info organisation="com.coveo" module="Abot" e:target="${msbuild.target}" e:platform="${msbuild.platform}">
        <ivyauthor name="coveo" url="http://www.coveo.com/"/>
        <description>Abot Crawler</description>
    </info>
    <configurations>
        <!-- Configuration for IvyResolve. DO NOT REMOVE. -->
        <conf name="all" extends="*"/>
    </configurations>
    <publications>
        <!-- Common artifacts -->
        <artifact name="Connectors/PostRetrieve" ext="proj" type="postretrieve" conf="*"/>
    </publications>
    <dependencies>
      <dependency org="org.moqteam" name="Moq" rev="4.2" />
      <dependency org="org.nunit" name="nunit" rev="2.6.4" />
      <dependency org="org.codeplex" name="HtmlAgilityPack" rev="1.4.9" />
      <dependency org="org.apache" name="log4net" rev="1.2.15" e:netversion="net-cp3.5" />
      <dependency org="org.automapper" name="AutoMapper" rev="3.3.1"/>
      <dependency org="org.jamietre" name="CsQuery" rev="1.3.4"/>
      <dependency org="org.sjdirect" name="NRobots" rev="1.0.5.0"/>
    </dependencies>
</ivy-module>
